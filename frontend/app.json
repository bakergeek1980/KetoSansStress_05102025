{
  "expo": {
    "name": "KetoSansStress",
    "slug": "keto-sans-stress",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ketosansstress",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ketosansstress.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ketosansstress.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow KetoSansStress to access your camera to scan meals."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow KetoSansStress to access your photo library to select meal images."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}